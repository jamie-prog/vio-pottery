  {
    "name": "Global",
    "settings": [
      {
        "type": "header",
        "content": "Quick View"
      },
      {
        "type": "checkbox",
        "id": "quickview",
        "label": "Enable quick view"
      },
      {
        "type": "checkbox",
        "id": "quick_show_payment_button",
        "label": "Show dynamic checkout button",
        "info": "Lets customers check out directly using a familiar payment method. [Learn more](https:\/\/help.shopify.com\/manual\/using-themes\/change-the-layout\/dynamic-checkout)",
        "default": true
      },
      {
        "type": "select",
        "id": "thumbnail_position",
        "label": "Thumbnail position",
        "options": [
          {
            "value": "right",
            "label": "Right"
          },
          {
            "value": "left",
            "label": "Left"
          },
          {
            "value": "bottom",
            "label": "Bottom"
          }
        ]
      },
      {
        "type": "checkbox",
        "id": "click_to_enlarge",
        "label": "Enable click to enlarge",
        "default": true
      },
      {
        "type": "checkbox",
        "id": "enable_video_looping",
        "label": "Enable video looping",
        "default": false
      },
      {
        "type": "paragraph",
        "content": "Learn more about [Media types](https:\/\/help.shopify.com\/en\/manual\/products\/product-media)"
      },
      {
        "type": "header",
        "content": "Product Grid"
      },
      {
        "type": "paragraph",
        "content": "These options control lists of products shown on the Featured Collection homepage section and the Collection Pages."
      },
      {
        "type": "checkbox",
        "id": "vendor",
        "label": "Show product vendor"
      },
      {
        "type": "select",
        "id": "details_align",
        "label": "Product detail text alignment",
        "options": [
          {
            "value": "left",
            "label": "Left"
          },
          {
            "value": "center",
            "label": "Center"
          },
          {
            "value": "right",
            "label": "Right"
          }
        ]
      },
      {
        "type": "radio",
        "id": "secondary_images_hover",
        "label": "Secondary image options:",
        "options": [
          {
            "value": "image_flip",
            "label": "On image hover"
          },
          {
            "value": "alternate_colors",
            "label": "On swatch hover"
          },
          {
            "value": "no_hover_effect",
            "label": "No secondary image"
          }
        ],
        "default": "no_hover_effect",
        "info": "On swatch hover requires an image to be assigned to a variant [See instructions](https:\/\/help.shopify.com\/manual\/products\/product-variant-images#add-images-to-existing-variants)"
      },
      {
        "type": "header",
        "content": "Keyboard Focus"
      },
      {
        "type": "paragraph",
        "content": "These options control the style of the keyboard focus"
      },
      {
        "type": "color",
        "id": "keyboard_focus",
        "label": "Border color",
        "default": "#007ace"
      },
      {
        "type": "select",
        "id": "focus_border_style",
        "label": "Border style",
        "default": "dashed",
        "options": [
          {
            "value": "dashed",
            "label": "Dashed"
          },
          {
            "value": "solid",
            "label": "Solid"
          },
          {
            "value": "dotted",
            "label": "Dotted"
          }
        ]
      },
      {
        "type": "range",
        "id": "focus_border_weight",
        "min": 1,
        "max": 3,
        "step": 1,
        "unit": "px",
        "label": "Border weight",
        "default": 2
      }
    ]
